
<div class="booking-page">
<%= link_to profile_path do %>
  <i class="fas fa-arrow-left"> Back To Profile</i>
<% end %>

<div class="container booking-container" style="margin:auto;">
  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="container">
        <h4 style="color: black; text-shadow: none;"><%= booking.car.title %></h4>
        <p><i class="fa fa-calendar"></i>  Starts: <%= booking.start_date %></p>
        <p><i class="fa fa-calendar"></i>  Starts: <%= booking.end_date %></p>
        <br>
        <p><i class="fa fa-envelope"></i>  Contact: <%= booking.car.user.email%></p>
        <br>
          <%= link_to booking_path(booking.id) do %>
          <p class="bkbtn">More Details</p>
          <% end %>
      </div>
    </div>
    <% end %>
  </div>
</div>












