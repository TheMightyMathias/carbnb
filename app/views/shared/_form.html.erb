<%= simple_form_for @car do |f| %>
<%= f.input :title %>
<%= f.input :brand %>
<%= f.input :model %>
<%= f.input :year %>
<%= f.input :color %>
<%= f.input :location %>
<%= f.input :price %>
<%= f.file_field  :photos, multiple: true %>
<%= f.input :url, label: "Picture URL" %>
<%= f.submit %>
<% end %>
